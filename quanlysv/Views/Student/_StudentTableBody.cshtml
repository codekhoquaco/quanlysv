@model IEnumerable<QuanLySinhVien.Models.Student>

@foreach (var s in Model)
{
    <tr>
        <td>@s.StudentID</td>
        <td>@s.LastName @s.FirstName</td>
        <td>@s.Gender</td>
        <td>@s.DateOfBirth.ToString("dd/MM/yyyy")</td>
        <td>@s.Email</td>
        <td>@s.PhoneNumber</td>
        <td>@s.ClassID</td>
        <td>@s.ClassName</td>
        <td>
            <a asp-action="Edit" asp-route-id="@s.StudentID" class="btn btn-sm btn-warning">Sửa</a>
            <a asp-action="Delete" asp-route-id="@s.StudentID" class="btn btn-sm btn-danger">Xóa</a>
        </td>
    </tr>
}